    funcFit->SetParName(1,"Mean");
    funcFit->SetParName(2,"Std Dev");
    funcFit->SetParName(3,"Noise");
    funcFit->SetParameter(0, 3600);
    funcFit->SetParameter(1, 1264);
    funcFit->SetParameter(2, 15);
    funcFit->SetParameter(3, 100);
